{
  "name": "bloggpt",
  "version": "1.0.0",
  "description": "Personal blog sharing platform",
  "main": "index.ts",
  "scripts": {
    "dev": "concurrently \" nodemon\" \"npm run client\"",
    "prod": "node --no-warnings --experimental-specifier-resolution=node --loader ./loader.js index.ts",
    "server": "nodemon",
    "client": "cd ../client/bloggpt && set PORT=3001 && npm start",
    "clean": "rimraf node_modules package-lock.json"
  },
  "keywords": [
    "blog",
    "gpt",
    "machine learning"
  ],
  "author": "w",
  "license": "MIT",
  "dependencies": {
    "@types/config": "^3.3.0",
    "@types/cors": "^2.8.13",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.17",
    "@types/express-session": "^1.17.7",
    "@types/node": "^18.8.3",
    "@types/passport": "^1.0.12",
    "@types/passport-google-oauth20": "^2.0.11",
    "@types/socket.io": "^3.0.2",
    "config": "^3.3.9",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-session": "^1.17.3",
    "mongodb": "^5.1.0",
    "mongoose": "^7.0.3",
    "passport": "^0.6.0",
    "puppeteer": "^18.2.1",
    "socket.io": "^4.6.1",
    "ts-node": "^10.9.1"
  },
  "devDependencies": {
    "concurrently": "^8.0.1",
    "nodemon": "^2.0.20",
    "rimraf": "^4.4.1",
    "typescript": "^4.8.4"
  },
  "type": "module"
}