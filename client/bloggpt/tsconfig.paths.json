{
  "compilerOptions": {
    "baseUrl": "./src",
    "paths": {
      "@view/*": [
        "component/*"
      ],
      "@control/*": [
        "controller/control/*"
      ],
      "@redux-action/*": [
        "controller/actions/*"
      ],
      "@redux-reducer/*": [
        "controller/reducers/*"
      ],
      "@src/*":["*"]
    }
  }
}